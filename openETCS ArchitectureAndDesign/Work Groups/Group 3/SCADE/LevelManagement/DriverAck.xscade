			</signals>
			<condition>
				<IdExpression>
					<path>
						<ConstVarRef name="_L15"/>
					</path>
				</IdExpression>
			</condition>
			<pragmas>
				<ed:Emission oid="!ed/18475/2CA1/350/54c6786c45bf"/>
			</pragmas>
		</Emission>
