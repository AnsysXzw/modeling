		<StateMachine name="Mode_Profile_By_Trackside">
			<states>
				<State initial="true" name="Procedure_Off">
					<unless>
						<Transition>
							<condition>
								<IdExpression>
									<path>
										<ConstVarRef name="Procedure_Possible"/>
									</path>
								</IdExpression>
							</condition>
							<effect>
								<Action>
									<pragmas>
										<ed:Action oid="!ed/22ab2/6615/BC4/54ae643c1668"/>
									</pragmas>
								</Action>
							</effect>
							<forks>
								<ForkBranch kind="restart">
									<target>
										<StateRef name="Futher_location"/>
									</target>
									<condition>
										<!-- Loc_Mode_Available and  not Loc_Mode_Profile_Condition -->
										<NAryOp name="26" operator="and">
											<operands>
												<IdExpression>
													<path>
														<ConstVarRef name="Loc_Mode_Available"/>
													</path>
												</IdExpression>
												<!--  not Loc_Mode_Profile_Condition -->
												<UnaryOp name="28" operator="not">
													<operand>
														<IdExpression>
															<path>
																<ConstVarRef name="Loc_Mode_Profile_Condition"/>
															</path>
														</IdExpression>
													</operand>
												</UnaryOp>
											</operands>
										</NAryOp>
									</condition>
									<pragmas>
										<ed:ForkBranch oid="!ed/39d5f/3351/9B8/54be72ca3084"/>
									</pragmas>
								</ForkBranch>
								<ForkBranch kind="restart">
									<target>
										<StateRef name="Current_Location"/>
									</target>
									<condition>
										<!-- Loc_Mode_Available and Loc_Mode_Profile_Condition -->
										<NAryOp name="24" operator="and">
											<operands>
												<IdExpression>
													<path>
														<ConstVarRef name="Loc_Mode_Available"/>
													</path>
												</IdExpression>
												<IdExpression>
													<path>
														<ConstVarRef name="Loc_Mode_Profile_Condition"/>
													</path>
												</IdExpression>
											</operands>
										</NAryOp>
									</condition>
									<pragmas>
										<ed:ForkBranch oid="!ed/39e7e/3351/9B8/54be733e43ce"/>
									</pragmas>
								</ForkBranch>
							</forks>
							<pragmas>
								<ed:Transition oid="!ed/22ab1/6615/BC4/54ae643c3c6d"/>
							</pragmas>
						</Transition>
					</unless>
					<pragmas>
						<ed:State oid="!ed/22ab0/6615/BC4/54ae643c2fd6"/>
					</pragmas>
				</State>
				<State name="Futher_location">
					<unless>
						<Transition kind="restart">
							<target>
								<StateRef name="Procedure_Off"/>
							</target>
							<condition>
								<!--  not Procedure_Possible -->
								<UnaryOp name="30" operator="not">
									<operand>
										<IdExpression>
											<path>
												<ConstVarRef name="Procedure_Possible"/>
											</path>
										</IdExpression>
									</operand>
								</UnaryOp>
							</condition>
							<effect>
								<Action>
									<pragmas>
										<ed:Action oid="!ed/22b18/6615/BC4/54ae643c26d1"/>
									</pragmas>
								</Action>
							</effect>
							<pragmas>
								<ed:Transition oid="!ed/22b17/6615/BC4/54ae643cf1f"/>
							</pragmas>
						</Transition>
						<Transition kind="restart">
							<target>
								<StateRef name="Current_Location"/>
							</target>
							<condition>
								<IdExpression>
									<path>
										<ConstVarRef name="Loc_Mode_Profile_Condition"/>
									</path>
								</IdExpression>
							</condition>
							<pragmas>
								<ed:Transition oid="!ed/39fa4/3351/9B8/54be8a156eae" xmlns="http://www.esterel-technologies.com/ns/scade/pragmas/editor/4">
									<comment>SRS § 5.7.3.6,, 5.9.3.7, 5.19.3.7</comment>
								</ed:Transition>
							</pragmas>
						</Transition>
					</unless>
					<data>
						<StateMachine name="Futher_Location">
							<states>
								<State initial="true" name="Req_Futher_Location">
									<unless>
										<Transition kind="restart">
											<target>
												<StateRef name="Start_Supervision"/>
											</target>
											<condition>
												<IdExpression>
													<path>
														<ConstVarRef name="Supervision_Modes"/>
													</path>
												</IdExpression>
											</condition>
											<effect>
												<Action>
													<pragmas>
														<ed:Action oid="!ed/39d24/3351/9B8/54be72647c41"/>
													</pragmas>
												</Action>
											</effect>
											<pragmas>
												<ed:Transition oid="!ed/39d23/3351/9B8/54be72646180" xmlns="http://www.esterel-technologies.com/ns/scade/pragmas/editor/4">
													<comment>SRS § 5.7.3.2,, 5.9.3.2, 5.19.3.2</comment>
												</ed:Transition>
											</pragmas>
										</Transition>
									</unless>
									<pragmas>
										<ed:State oid="!ed/39d22/3351/9B8/54be72641a11"/>
									</pragmas>
								</State>
								<State name="Start_Supervision">
									<unless>
										<Transition kind="restart">
											<target>
												<StateRef name="Waiting_Ack"/>
											</target>
											<condition>
												<IdExpression>
													<path>
														<ConstVarRef name="Loc_Supervision_Condition_Covered"/>
													</path>
												</IdExpression>
											</condition>
											<effect>
												<Action>
													<pragmas>
														<ed:Action oid="!ed/39d27/3351/9B8/54be7264642b"/>
													</pragmas>
												</Action>
											</effect>
											<pragmas>
												<ed:Transition oid="!ed/39d26/3351/9B8/54be7264450c" xmlns="http://www.esterel-technologies.com/ns/scade/pragmas/editor/4">
													<comment>SRS § 5.7.3.2,, 5.9.3.2, 5.19.3.2</comment>
												</ed:Transition>
											</pragmas>
										</Transition>
									</unless>
									<pragmas>
										<ed:State oid="!ed/39d25/3351/9B8/54be72643fc6"/>
									</pragmas>
								</State>
								<State name="Waiting_Ack">
									<unless>
										<Transition kind="restart">
											<target>
												<StateRef name="Switch_Mode_Confirmed"/>
											</target>
											<condition>
												<IdExpression>
													<path>
														<ConstVarRef name="Driver_Ack_Mode_Profile"/>
													</path>
												</IdExpression>
											</condition>
											<effect>
												<Action>
													<pragmas>
														<ed:Action oid="!ed/39d32/3351/9B8/54be72641698"/>
													</pragmas>
												</Action>
