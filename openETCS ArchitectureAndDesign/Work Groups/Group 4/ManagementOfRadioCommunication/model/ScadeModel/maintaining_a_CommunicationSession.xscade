<?xml version="1.0" encoding="UTF-8"?>
<File xmlns="http://www.esterel-technologies.com/ns/scade/4" xmlns:ed="http://www.esterel-technologies.com/ns/scade/pragmas/editor/4" xmlns:kcg="http://www.esterel-technologies.com/ns/scade/pragmas/codegen/2">
	<declarations>
		<Operator kind="node" visibility="private" name="maintaining_a_CommunicationSession">
			<inputs>
				<Variable name="communicationSessionEstablished">
					<type>
						<NamedType>
							<type>
								<TypeRef name="bool"/>
							</type>
						</NamedType>
					</type>
					<pragmas>
						<ed:Variable oid="!ed/85d4/7691/2620/50928c7e4e36" xmlns="http://www.esterel-technologies.com/ns/scade/pragmas/editor/4">
							<comment>3.5.4.1</comment>
						</ed:Variable>
					</pragmas>
				</Variable>
				<Variable name="safeRadioConnectionReestablished">
					<type>
						<NamedType>
							<type>
								<TypeRef name="bool"/>
							</type>
						</NamedType>
					</type>
					<pragmas>
						<ed:Variable oid="!ed/22c7/4931/1928/50a0f70335ca" xmlns="http://www.esterel-technologies.com/ns/scade/pragmas/editor/4">
							<comment>3.5.4.1</comment>
						</ed:Variable>
					</pragmas>
				</Variable>
				<Variable name="lossOfTheSafeRadioConnection">
					<type>
						<NamedType>
							<type>
								<TypeRef name="bool"/>
							</type>
						</NamedType>
					</type>
					<pragmas>
						<ed:Variable oid="!ed/85d5/7691/2620/50928c936b6c" xmlns="http://www.esterel-technologies.com/ns/scade/pragmas/editor/4">
							<comment>3.5.4.1</comment>
						</ed:Variable>
					</pragmas>
				</Variable>
				<Variable name="disconnectionHasNotBeenOrdered">
					<type>
						<NamedType>
							<type>
								<TypeRef name="bool"/>
							</type>
						</NamedType>
					</type>
					<pragmas>
						<ed:Variable oid="!ed/85d6/7691/2620/50928cbe2735" xmlns="http://www.esterel-technologies.com/ns/scade/pragmas/editor/4">
							<comment>3.5.4.1</comment>
						</ed:Variable>
					</pragmas>
				</Variable>
				<Variable name="trainFrontInsideInAnAnnouncedRadioHole">
					<type>
						<NamedType>
							<type>
								<TypeRef name="bool"/>
							</type>
						</NamedType>
					</type>
					<pragmas>
						<ed:Variable oid="!ed/2313/4931/1928/50a10ee02c8" xmlns="http://www.esterel-technologies.com/ns/scade/pragmas/editor/4">
							<comment>3.5.4.4</comment>
						</ed:Variable>
					</pragmas>
				</Variable>
				<Variable name="actualTime">
					<type>
						<NamedType>
							<type>
								<TypeRef name="time_Type"/>
							</type>
						</NamedType>
					</type>
					<pragmas>
						<ed:Variable oid="!ed/23f7/79C7/1248/50a204f4592a"/>
					</pragmas>
				</Variable>
			</inputs>
			<outputs>
				<Variable name="tryToSetupANewSafeRadioConnection">
					<type>
						<NamedType>
							<type>
								<TypeRef name="bool"/>
							</type>
						</NamedType>
					</type>
					<default>
						<ConstValue value="false"/>
					</default>
					<pragmas>
						<ed:Variable oid="!ed/235f/4931/1928/50a111553ff"/>
					</pragmas>
				</Variable>
				<Variable name="informTheDriverThatNoConnectionWasSetup">
					<type>
						<NamedType>
							<type>
								<TypeRef name="bool"/>
							</type>
						</NamedType>
					</type>
					<pragmas>
						<ed:Variable oid="!ed/2456/79C7/1248/50a209a82ad0"/>
					</pragmas>
				</Variable>
				<Variable name="firstRequestToSetupASafeRadioConnection">
					<type>
						<NamedType>
							<type>
								<TypeRef name="bool"/>
							</type>
						</NamedType>
					</type>
					<pragmas>
						<ed:Variable oid="!ed/298a/628D/11FC/50f51c5e1879" xmlns="http://www.esterel-technologies.com/ns/scade/pragmas/editor/4">
							<comment>3.5.7.3b)</comment>
						</ed:Variable>
					</pragmas>
				</Variable>
				<Variable name="finalAttemptToSetupTheSafeRadioConnectionFailed">
					<type>
						<NamedType>
							<type>
								<TypeRef name="bool"/>
							</type>
						</NamedType>
					</type>
					<pragmas>
						<ed:Variable oid="!ed/2990/628D/11FC/50f51c5e1d90" xmlns="http://www.esterel-technologies.com/ns/scade/pragmas/editor/4">
							<comment>3.5.7.5[6]</comment>
						</ed:Variable>
					</pragmas>
				</Variable>
				<Variable name="requestsToSetupTheSafeRadioConnectionStopped">
					<type>
						<NamedType>
							<type>
								<TypeRef name="bool"/>
							</type>
						</NamedType>
					</type>
					<pragmas>
						<ed:Variable oid="!ed/2996/628D/11FC/50f51c5e52ca" xmlns="http://www.esterel-technologies.com/ns/scade/pragmas/editor/4">
							<comment>3.5.7.4</comment>
						</ed:Variable>
					</pragmas>
				</Variable>
			</outputs>
			<signals>
				<Signal name="sInformTheDriverThatNoConnectionWasSetup">
					<pragmas>
						<ed:Signal oid="!ed/2443/79C7/1248/50a2097a35d4"/>
					</pragmas>
				</Signal>
				<Signal name="sStartToTryToSetupASafeRadioConnection">
					<pragmas>
						<ed:Signal oid="!ed/29aa/628D/11FC/50f51e996814" xmlns="http://www.esterel-technologies.com/ns/scade/pragmas/editor/4">
							<comment>3.5.7.3b)</comment>
						</ed:Signal>
					</pragmas>
				</Signal>
				<Signal name="sFinalAttemptToSetupTheSafeRadioConnectionFailed">
					<pragmas>
						<ed:Signal oid="!ed/29c7/628D/11FC/50f520532b23"/>
					</pragmas>
				</Signal>
				<Signal name="sConnecctionLostInAnnouncedRadioHole">
					<pragmas>
						<ed:Signal oid="!ed/2a07/628D/11FC/50f523b16f79"/>
					</pragmas>
				</Signal>
			</signals>
			<locals>
				<Variable name="_L1">
					<type>
						<NamedType>
							<type>
								<TypeRef name="bool"/>
							</type>
						</NamedType>
					</type>
					<pragmas>
						<ed:Variable oid="!ed/2451/79C7/1248/50a2099a6a41"/>
					</pragmas>
				</Variable>
				<Variable name="_L2">
					<type>
						<NamedType>
							<type>
								<TypeRef name="bool"/>
							</type>
						</NamedType>
					</type>
					<pragmas>
						<ed:Variable oid="!ed/29be/628D/11FC/50f51f815a61"/>
					</pragmas>
				</Variable>
				<Variable name="_L3">
					<type>
						<NamedType>
							<type>
								<TypeRef name="bool"/>
							</type>
						</NamedType>
					</type>
					<pragmas>
						<ed:Variable oid="!ed/29d5/628D/11FC/50f520866bf8"/>
					</pragmas>
				</Variable>
				<Variable name="_L4">
					<type>
						<NamedType>
							<type>
								<TypeRef name="bool"/>
							</type>
						</NamedType>
					</type>
					<pragmas>
						<ed:Variable oid="!ed/29fb/628D/11FC/50f5227d44f7"/>
					</pragmas>
				</Variable>
				<Variable name="_L5">
					<type>
						<NamedType>
							<type>
								<TypeRef name="bool"/>
							</type>
						</NamedType>
					</type>
					<pragmas>
						<ed:Variable oid="!ed/2a16/628D/11FC/50f523e948e5"/>
					</pragmas>
				</Variable>
			</locals>
			<data>
				<StateMachine name="Maintaining_SM">
					<states>
						<State initial="true" name="CommunicationSessionNotEstablished">
							<unless>
								<Transition kind="restart">
									<target>
										<StateRef name="CommunicationSessionEstablished"/>
									</target>
									<condition>
										<IdExpression>
											<path>
												<ConstVarRef name="communicationSessionEstablished"/>
											</path>
										</IdExpression>
									</condition>
									<pragmas>
										<ed:Transition oid="!ed/85f7/7691/2620/50928dd463e5" xmlns="http://www.esterel-technologies.com/ns/scade/pragmas/editor/4">
											<comment>3.5.4.1</comment>
										</ed:Transition>
									</pragmas>
								</Transition>
							</unless>
							<pragmas>
								<ed:State oid="!ed/85d8/7691/2620/50928d3633a6"/>
							</pragmas>
						</State>
						<State name="CommunicationSessionEstablished">
							<signals>
								<Signal name="sStart_RadioConnectionLostTimer">
									<pragmas>
										<ed:Signal oid="!ed/866d/7691/2620/509291cc43a9"/>
									</pragmas>
								</Signal>
							</signals>
							<locals>
								<Variable name="radioConnectionLostTimer_expired">
									<type>
										<NamedType>
											<type>
												<TypeRef name="bool"/>
											</type>
										</NamedType>
									</type>
									<pragmas>
										<ed:Variable oid="!ed/867c/7691/2620/509292237550"/>
									</pragmas>
								</Variable>
								<Variable name="_L2">
									<type>
										<NamedType>
											<type>
												<TypeRef name="bool"/>
											</type>
										</NamedType>
									</type>
									<pragmas>
										<ed:Variable oid="!ed/2375/4931/1928/50a111ea68c3"/>
									</pragmas>
								</Variable>
								<Variable name="_L3">
									<type>
										<NamedType>
											<type>
												<TypeRef name="bool"/>
											</type>
										</NamedType>
									</type>
									<pragmas>
										<ed:Variable oid="!ed/23c8/79C7/1248/50a203a771f2"/>
									</pragmas>
								</Variable>
								<Variable name="_L4">
									<type>
										<NamedType>
											<type>
												<TypeRef name="bool"/>
											</type>
										</NamedType>
									</type>
									<pragmas>
										<ed:Variable oid="!ed/23ea/79C7/1248/50a204c8439c"/>
									</pragmas>
								</Variable>
								<Variable name="_L5">
									<type>
										<NamedType>
											<type>
												<TypeRef name="time_Type"/>
											</type>
										</NamedType>
									</type>
									<pragmas>
										<ed:Variable oid="!ed/23f9/79C7/1248/50a204fc4cad"/>
									</pragmas>
								</Variable>
								<Variable name="_L6">
									<type>
										<NamedType>
											<type>
												<TypeRef name="time_Type"/>
											</type>
										</NamedType>
									</type>
									<pragmas>
										<ed:Variable oid="!ed/2409/79C7/1248/50a2065e4ea3"/>
									</pragmas>
								</Variable>
								<Variable name="_L7">
									<type>
										<NamedType>
											<type>
												<TypeRef name="bool"/>
											</type>
										</NamedType>
									</type>
									<pragmas>
										<ed:Variable oid="!ed/2538/2AF/250/50c5b4fe7f55"/>
									</pragmas>
								</Variable>
							</locals>
							<data>
								<StateMachine name="RadioConnection_SM">
									<states>
										<State initial="true" name="SafeRadio_connected">
											<unless>
												<Transition kind="restart">
													<target>
														<StateRef name="ConnecctionLostInAnnouncedRadioHole"/>
													</target>
													<condition>
														<!-- trainFrontInsideInAnAnnouncedRadioHole and lossOfTheSafeRadioConnection -->
														<NAryOp name="2" operator="and">
															<operands>
																<IdExpression>
																	<path>
																		<ConstVarRef name="trainFrontInsideInAnAnnouncedRadioHole"/>
																	</path>
																</IdExpression>
																<IdExpression>
																	<path>
																		<ConstVarRef name="lossOfTheSafeRadioConnection"/>
																	</path>
																</IdExpression>
															</operands>
														</NAryOp>
													</condition>
													<effect>
														<Action>
															<data>
																<!--  emit 'sConnecctionLostInAnnouncedRadioHole; -->
																<Emission>
																	<signals>
																		<SignalRef name="sConnecctionLostInAnnouncedRadioHole"/>
																	</signals>
																	<pragmas>
																		<ed:Emission oid="!ed/2a13/628D/11FC/50f523cd1dcc"/>
																	</pragmas>
																</Emission>
															</data>
															<pragmas>
																<ed:Action oid="!ed/2a09/628D/11FC/50f523cd659b"/>
															</pragmas>
														</Action>
													</effect>
													<pragmas>
														<ed:Transition oid="!ed/2309/4931/1928/50a0f9e23d2b"/>
													</pragmas>
												</Transition>
												<Transition kind="restart">
													<target>
														<StateRef name="ConnectionLost"/>
													</target>
													<condition>
														<!-- lossOfTheSafeRadioConnection and disconnectionHasNotBeenOrdered -->
														<NAryOp name="1" operator="and">
															<operands>
																<IdExpression>
																	<path>
																		<ConstVarRef name="lossOfTheSafeRadioConnection"/>
																	</path>
																</IdExpression>
																<IdExpression>
																	<path>
																		<ConstVarRef name="disconnectionHasNotBeenOrdered"/>
																	</path>
																</IdExpression>
															</operands>
														</NAryOp>
													</condition>
													<effect>
														<Action>
															<data>
																<!--  emit 'sStart_RadioConnectionLostTimer; -->
																<Emission>
																	<signals>
																		<SignalRef name="sStart_RadioConnectionLostTimer"/>
																	</signals>
																	<pragmas>
																		<ed:Emission oid="!ed/29b8/628D/11FC/50f51f473883"/>
																	</pragmas>
																</Emission>
																<!--  emit 'sStartToTryToSetupASafeRadioConnection; -->
																<Emission>
																	<signals>
																		<SignalRef name="sStartToTryToSetupASafeRadioConnection"/>
																	</signals>
																	<pragmas>
																		<ed:Emission oid="!ed/29b9/628D/11FC/50f51f4732eb"/>
																	</pragmas>
																</Emission>
															</data>
															<pragmas>
																<ed:Action oid="!ed/8643/7691/2620/5092902814d9"/>
															</pragmas>
														</Action>
													</effect>
													<pragmas>
														<ed:Transition oid="!ed/862a/7691/2620/50928f74381d" xmlns="http://www.esterel-technologies.com/ns/scade/pragmas/editor/4">
															<comment>3.5.4.1</comment>
														</ed:Transition>
													</pragmas>
												</Transition>
											</unless>
											<pragmas>
												<ed:State oid="!ed/860e/7691/2620/50928f0b5bdc"/>
											</pragmas>
										</State>
										<State name="ConnectionLost">
											<unless>
												<Transition kind="restart">
													<target>
														<StateRef name="SafeRadio_connected"/>
													</target>
													<condition>
														<IdExpression>
															<path>
																<ConstVarRef name="safeRadioConnectionReestablished"/>
															</path>
														</IdExpression>
													</condition>
													<pragmas>
														<ed:Transition oid="!ed/22be/4931/1928/50a0f6e0e35" xmlns="http://www.esterel-technologies.com/ns/scade/pragmas/editor/4">
															<comment>3.5.4.3</comment>
														</ed:Transition>
													</pragmas>
												</Transition>
												<Transition kind="restart">
													<target>
														<StateRef name="RadioConnectionLostTimerExpired"/>
													</target>
													<condition>
														<IdExpression>
															<path>
																<ConstVarRef name="radioConnectionLostTimer_expired"/>
															</path>
														</IdExpression>
													</condition>
													<pragmas>
														<ed:Transition oid="!ed/866a/7691/2620/5092918ad66" xmlns="http://www.esterel-technologies.com/ns/scade/pragmas/editor/4">
															<comment>3.5.4.2.1, 3.5.4.3</comment>
														</ed:Transition>
													</pragmas>
												</Transition>
											</unless>
											<locals>
												<Variable name="_L1">
													<type>
														<NamedType>
															<type>
																<TypeRef name="bool"/>
															</type>
														</NamedType>
													</type>
													<pragmas>
														<ed:Variable oid="!ed/228e/4931/1928/50a0f5581e4c"/>
													</pragmas>
												</Variable>
											</locals>
											<data>
												<!-- _L1 = true; -->
												<Equation>
													<lefts>
														<VariableRef name="_L1"/>
													</lefts>
													<right>
														<ConstValue value="true"/>
													</right>
													<pragmas>
														<ed:Equation oid="!ed/228d/4931/1928/50a0f5581913"/>
													</pragmas>
												</Equation>
												<!-- tryToSetupANewSafeRadioConnection = _L1; -->
												<Equation>
													<lefts>
														<VariableRef name="tryToSetupANewSafeRadioConnection"/>
													</lefts>
													<right>
														<IdExpression>
															<path>
																<ConstVarRef name="_L1"/>
															</path>
														</IdExpression>
													</right>
													<pragmas>
														<ed:Equation oid="!ed/2360/4931/1928/50a1116371ce"/>
													</pragmas>
												</Equation>
											</data>
											<pragmas>
												<ed:State oid="!ed/861b/7691/2620/50928f526ec3"/>
											</pragmas>
										</State>
										<State final="true" name="RadioConnectionLostTimerExpired">
											<pragmas>
												<ed:State oid="!ed/8656/7691/2620/5092912a5b01"/>
											</pragmas>
										</State>
										<State name="ConnecctionLostInAnnouncedRadioHole">
											<unless>
												<Transition kind="restart">
													<target>
														<StateRef name="ConnectionLost"/>
													</target>
													<condition>
														<!--  not trainFrontInsideInAnAnnouncedRadioHole -->
														<UnaryOp name="1" operator="not">
															<operand>
																<IdExpression>
																	<path>
																		<ConstVarRef name="trainFrontInsideInAnAnnouncedRadioHole"/>
																	</path>
																</IdExpression>
															</operand>
														</UnaryOp>
													</condition>
													<effect>
														<Action>
															<data>
																<!--  emit 'sStart_RadioConnectionLostTimer; -->
																<Emission>
																	<signals>
																		<SignalRef name="sStart_RadioConnectionLostTimer"/>
																	</signals>
																	<pragmas>
																		<ed:Emission oid="!ed/29ea/628D/11FC/50f522082b71"/>
																	</pragmas>
																</Emission>
																<!--  emit 'sStartToTryToSetupASafeRadioConnection; -->
																<Emission>
																	<signals>
																		<SignalRef name="sStartToTryToSetupASafeRadioConnection"/>
																	</signals>
																	<pragmas>
																		<ed:Emission oid="!ed/29eb/628D/11FC/50f522081a3d"/>
																	</pragmas>
																</Emission>
															</data>
															<pragmas>
																<ed:Action oid="!ed/234b/4931/1928/50a110145feb"/>
															</pragmas>
														</Action>
													</effect>
													<pragmas>
														<ed:Transition oid="!ed/2338/4931/1928/50a10fb151e8" xmlns="http://www.esterel-technologies.com/ns/scade/pragmas/editor/4">
															<comment>3.5.4.4</comment>
														</ed:Transition>
													</pragmas>
												</Transition>
											</unless>
											<pragmas>
												<ed:State oid="!ed/22ff/4931/1928/50a0f9ae3457" xmlns="http://www.esterel-technologies.com/ns/scade/pragmas/editor/4">
													<comment>3.5.4.4</comment>
												</ed:State>
											</pragmas>
										</State>
									</states>
									<pragmas>
										<ed:StateMachine oid="!ed/8608/7691/2620/50928ed75d6"/>
									</pragmas>
								</StateMachine>
								<!-- radioConnectionLostTimer_expired = _L3; -->
								<Equation>
									<lefts>
										<VariableRef name="radioConnectionLostTimer_expired"/>
									</lefts>
									<right>
										<IdExpression>
											<path>
												<ConstVarRef name="_L3"/>
											</path>
										</IdExpression>
									</right>
									<pragmas>
										<ed:Equation oid="!ed/868a/7691/2620/509292727053"/>
									</pragmas>
								</Equation>
								<!-- _L2 =  'sStart_RadioConnectionLostTimer; -->
								<Equation>
									<lefts>
										<VariableRef name="_L2"/>
									</lefts>
									<right>
										<Present>
											<signal>
												<SignalRef name="sStart_RadioConnectionLostTimer"/>
											</signal>
										</Present>
									</right>
									<pragmas>
										<ed:Equation oid="!ed/2374/4931/1928/50a111ea38c"/>
									</pragmas>
