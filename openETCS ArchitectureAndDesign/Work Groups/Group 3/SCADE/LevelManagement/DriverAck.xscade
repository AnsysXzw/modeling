			</lefts>
			<right>
				<!-- _L10 >= _L11 -->
				<BinaryOp name="1" operator="&gt;=">
					<operands>
						<IdExpression>
							<path>
								<ConstVarRef name="_L10"/>
							</path>
						</IdExpression>
						<IdExpression>
							<path>
								<ConstVarRef name="_L11"/>
							</path>
						</IdExpression>
					</operands>
				</BinaryOp>
			</right>
			<pragmas>
				<ed:Equation oid="!ed/18474/2CA1/350/54c6786c11b7"/>
			</pragmas>
		</Equation>
		<!--  emit 'passAckDistance if _L15; -->
		<Emission>
			<signals>
